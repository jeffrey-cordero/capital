{
  "name": "capital-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx tsc && npx tsc-alias --resolve-full-paths",
    "start": "npm run build && NODE_ENV=production node --no-warnings=ExperimentalWarning build/api/index.js",
    "dev": "nodemon -L --watch '**/*.ts' --ext ts --exec 'npm run build && node build/api/index.js'",
    "lint": "npx eslint '**/*.ts' --ignore-pattern '**/build/'",
    "lint:fix": "npx eslint '**/*.ts' --fix --ignore-pattern '**/build/'",
    "test": "npx jest tests/* --collect-coverage --detectOpenHandles --verbose"
  },
  "keywords": [],
  "author": "Jeffrey Cordero",
  "license": "MIT",
  "dependencies": {
    "argon2": "^0.41.1",
    "async-mutex": "^0.5.0",
    "body-parser": "^1.20.2",
    "capital": "file:../types",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.0",
    "fs": "^0.0.1-security",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
